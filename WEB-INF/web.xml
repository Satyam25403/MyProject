<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>WebPageRegistration</display-name>
  <welcome-file-list>
    <welcome-file>signup.html</welcome-file>
   
  </welcome-file-list>
  
  <servlet>
    <servlet-name>Signup</servlet-name>
    <servlet-class>Signup</servlet-class>
  </servlet>
  <servlet-mapping>

    <servlet-name>Signup</servlet-name>
    <url-pattern>/Signup</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>FilterResults</servlet-name>
    <servlet-class>FilterResults</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FilterResults</servlet-name>
    <url-pattern>/FilterResults</url-pattern>
  </servlet-mapping>
</web-app>